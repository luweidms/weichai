<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youming.youche.record.provider.mapper.tenant.TenantVehicleCostRelMapper">
   <select id="getTenantVehicleCostRelList" resultType="com.youming.youche.record.domain.tenant.TenantVehicleCostRel">
       select * from tenant_vehicle_cost_rel where VEHICLE_CODE = #{vehicleCode} and TENANT_ID = #{tenantId}
   </select>
</mapper>
